{
  "Traceyi": {
    "Tracer": [
      {
        "UseMemberInfo": false,
        "Listeners": [ "console" ]
      }
    ],
    "Listener": {
      "Console": [
        {
          "Name": "console",
          "UseLock": true,
          "UseAsync": false,
          "Layout": {
            "Format": "{operationId} {action|,10:L}{message}"
          }
        }
      ]
    }
  }
}
