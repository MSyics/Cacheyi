{
  "Traceyi": {
    "Tracer": [
      {
        "Listeners": [ "console", "file" ]
      }
    ],
    "Listener": {
      "Console": [
        {
          "Name": "console",
          "Layout": {
            "Format": "{dateTime:yyyy/MM/dd} {dateTime:HH:mm:ss.ffff}{tab}{action}{tab}{class}.{member}{tab}{message}"
          }
        }
      ],
      "File": [
        {
          "Name": "file",
          "Layout": {
            "Format": "{dateTime:yyyy/MM/dd} {dateTime:HH:mm:ss.ffff}{tab}{action}{tab}{class}.{member}{tab}{message}"
          }
        }
      ]
    }
  }
}
