{
  "Traceyi": {
    "Tracer": [
      {
        "Listeners": [ "console" ]
      }
    ],
    "Listener": {
      "Console": [
        {
          "Name": "console",
          "UseLock": false,
          "UseAsync": true,
          "Layout": {
            "Format": "{elapsed:hh\\.mm\\.ss\\.fffffff} {action|,10:L}{operationId}{tab}{message}"
          }
        }
      ]
    }
  }
}
