<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>2.4.0</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/MSyics/Cacheyi</PackageProjectUrl>
    <RepositoryUrl>https://github.com/MSyics/Cacheyi</RepositoryUrl>
    <PackageTags>cache</PackageTags>
    <PackageReleaseNotes></PackageReleaseNotes>
    <PackageLicenseUrl></PackageLicenseUrl>
    <Description>Cacheyi is a micro cache management library. by fluent API.</Description>
    <Copyright>Copyright (c) 2019 MSyics</Copyright>
    <Company></Company>
    <Authors>MSyics</Authors>
    <PackageId>Cacheyi</PackageId>
    <Product>MSyics.Cacheyi</Product>
    <AssemblyVersion>2.4.0.0</AssemblyVersion>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>bin\Debug\netstandard2.0\MSyics.Cacheyi.xml</DocumentationFile>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DocumentationFile>bin\Release\netstandard2.0\MSyics.Cacheyi.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="CacheStore - コピー.cs" />
  </ItemGroup>

</Project>
